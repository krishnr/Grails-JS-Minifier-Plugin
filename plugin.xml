<plugin name='js-plugin' version='3.4' grailsVersion='2.2 &gt; *'>
  <author>Krishn Ramesh</author>
  <title>JS Plugin</title>
  <description>

This plugin uses Google's Closure Compiler to minify JS files upon building a WAR
</description>
  <documentation>http://grails.org/plugin/js-plugin</documentation>
  <type>JSpluginGrailsPlugin</type>
  <resources>
    <resource>jsplugin.JSTagLib</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='' name='junit' version='3.8.1' />
      <dependency group='' name='args4j' version='2.0.16' />
      <dependency group='' name='caja' version='r4939' />
      <dependency group='' name='guava' version='15.0' />
      <dependency group='' name='json-simple' version='1.1' />
      <dependency group='' name='closure-compiler' version='v20131014' />
      <dependency group='' name='xml-resolver' version='1.2' />
      <dependency group='' name='xml-apis' version='1.3.04' />
      <dependency group='' name='javax.inject' version='1' />
      <dependency group='' name='xmlParserAPIs' version='2.6.2' />
      <dependency group='' name='protobuf-java' version='2.5.0' />
      <dependency group='' name='json' version='20090211' />
      <dependency group='' name='jsr305' version='2.0.3' />
    </compile>
  </dependencies>
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>